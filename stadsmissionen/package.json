    "clsx": "^2.1.1",
    "embla-carousel-vue": "^8.6.0",
    "lucide-vue-next": "^0.513.0",
    "pinia": "^3.0.3",
    "reka-ui": "^2.3.1",
    "tailwind-merge": "^3.3.0",
    "tw-animate-css": "^1.3.4",
    "vaul-vue": "^0.4.1",
    "vee-validate": "^4.15.0",
    "vue": "^3.5.16",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
